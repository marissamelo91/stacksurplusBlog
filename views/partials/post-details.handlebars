<section>
    <a href="/post/{{id}}"> 
        <div class="container">
            <div class="header">
                <h2>{{title}}</h2>
                <span>Posted by {{users.username}} {{formatDate createdAt}}</span>
            </div>
            <div class="body">
                <p>{{content}} </p>
            </div>

        </div>
    </a>

</section>